    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo.png",
        "backgroundColor": "#202120"
      },
      "package": "com.just_manoj_20.quotesTime"
    },
    "web": {
      "favicon": "./assets/Logo.png"
    }
  }
}
