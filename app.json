{
  "expo": {
    "name": "Quotes Time",
    "slug": "quotesTime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SplashImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#202120"
    },
    "updates": {
